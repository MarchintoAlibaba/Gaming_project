page{
  height: 100%;
  overflow: hidden;
}
.bg{
  width: 100%;
  height: 100%;
}
.bg_1{
  position: absolute;
  z-index: 1;
  top: 150rpx;
  left: 55rpx;
  width: 640rpx;
  height: 340rpx;
}
.time{
  width: 140rpx;
  height: 200rpx;
  position: absolute;
  top: 30rpx;
  left: 305rpx;
  z-index: 2
}
.time_num{
  position: absolute;
  z-index: 3;
  top: 130rpx;
  width: 100%;
  left: 0;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  color: #401894
}
.problem_num{
  position: absolute;
  z-index: 3;
  width: 100%;
  text-align: center;
  left: 0;
  top: 250rpx; 
  color: #fff;
  font-size: 32rpx;
}
.problem{
  position: absolute;
  top: 320rpx;
  left: 125rpx;
  width: 500rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  z-index: 3
}
.item_bg{
  width: 350rpx;
  height: 90rpx;
}
.answer{
  position: absolute;
  top: 550rpx;
  left: 200rpx;
  width: 350rpx;
  z-index: 1;
}
.an_item{
  margin-bottom: 30rpx;
  width: 350rpx;
  height: 90rpx;
  position: relative
}
.item_zh{
  position: absolute;
  width: 350rpx;
  height: 90rpx;
  top: 0;
  left: 0;
  line-height: 90rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.list_bg{
  position: absolute;
  top: 580rpx;
  left: 55rpx;
  width: 640rpx;
  height: 440rpx;
  z-index: 2;
}
.list{
  position: absolute;
  top: 640rpx;
  left: 90rpx;
  width: 570rpx;
  height: 360rpx;
  z-index: 1;
}
.list_container{
  position: absolute;
  top: 40rpx;
  left: 20rpx;
  width: 530rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}
.list_di{
  width: 100%;
  height: 100%;
}
.list_item{
  height: 40rpx;
  border: 1rpx solid #ee62ff;
  border-radius: 19rpx;
  background: #fff;
  display: flex;
  justify-content: start;
  align-items: center;
  font-size: 20rpx;
  width: 250rpx;
  margin-bottom: 20rpx;
}
.face{
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #ee62ff;
  margin-right: 5rpx;
}
.name{
  width: 75rpx;
  margin-left: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.score{
  color: #ee62ff;
  margin: 0 10rpx;
}
.yes{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: 28rpx;
  left: 30rpx;
}
.no{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 28rpx;
  left: 30rpx;
}